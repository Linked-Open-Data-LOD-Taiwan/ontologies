   "source": [
    "# 用 set 來比較兩個 array\n",
    "a=[1,2,3]\n",
    "b=[3,4,5,4]\n",
    "\n",
    "print(set(a).intersection(set(b))) # 交集\n",
    "print(set(a).union(set(b))) # 並集\n",
    "print(set(a).difference(set(b))) # 差集，在a中但不在b中的元素\n",
    "print(set(b).difference(set(a))) # 差集，在b中但不在a中的元素"
   ]
